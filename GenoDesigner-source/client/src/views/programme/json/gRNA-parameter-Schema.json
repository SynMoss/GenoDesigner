                },
                "option_gc_of_primer": {
                    "title": "option_gc_of_primer",
                    "type": "integer",
                    "default": 50,
                    "required":true
                },
                "min_tm_of_primer": {
                    "title": "min_tm_of_primer",
                    "type": "integer",
                    "default": 37,
                    "required":true
                },
                "max_tm_of_primer": {
                    "title": "max_tm_of_primer",
                    "type": "integer",
                    "default": 90,
