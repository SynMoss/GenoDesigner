                },
                "Configure": {
                    "title": "Configure",
                    "type": "string",
                    "required":true
                },
                "Outdir": {
                    "title": "Outdir",
                    "type": "string",
                    "default": "",
                    "required":true
                },
                "Left_Flank": {
                    "title": "Left_Flank",
                    "type": "integer",
                    "default": 2000,
                    "required":true
                },
                "Right_Flank": {
                    "title": "Right_Flank",
                    "type": "integer",
                    "default": 2000,
                    "required":true
                },
                "Extend_Size": {
                    "title": "Extend_Size",
                    "type": "integer",
                    "default": 100,
                    "required":true
