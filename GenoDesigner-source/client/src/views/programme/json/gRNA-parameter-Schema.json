                "gRNA_num": {
                    "title": "gRNA_num",
                    "type": "integer",
                    "default": 3,
                    "required":true
                },
                "Max_offtargets": {
                    "title": "Max_offtargets",
                    "type": "integer",
                    "default": 300,
                    "required":true
                },
                "Max_mismatches": {
                    "title": "Max_mismatches",
                    "type": "integer",
